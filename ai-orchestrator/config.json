{
    "name": "AI Orchestrator",
    "version": "0.8.25",
    "slug": "ai-orchestrator",
    "description": "Phase 6: Universal Agents, RAG, and No-Code Factory for Home Assistant",
    "url": "https://github.com/ITSpecialist111/HASS-AI-Orchestrator",
    "arch": [
        "amd64",
        "armv7",
        "aarch64"
    ],
    "startup": "application",
    "boot": "auto",
    "init": false,
    "ingress": true,
    "ingress_port": 8999,
    "ingress_entry": "/",
    "panel_icon": "mdi:robot",
    "ports": {
        "8999/tcp": 8999,
        "3654/tcp": 3654
    },
    "ports_description": {
        "8999/tcp": "Backend API and Dashboard",
        "3654/tcp": "Development dashboard server (optional)"
    },
    "map": [
        "addon_config:rw"
    ],
    "hassio_api": true,
    "hassio_role": "default",
    "homeassistant_api": true,
    "options": {
        "ollama_host": "http://localhost:11434",
        "dry_run_mode": true,
        "log_level": "info",
        "heating_model": "mistral:7b-instruct",
        "heating_entities": [],
        "ha_access_token": "",
        "decision_interval": 120,
        "enable_gpu": false
    },
    "schema": {
        "ollama_host": "str",
        "dry_run_mode": "bool",
        "log_level": "list(debug|info|warning|error)",
        "heating_model": "str",
        "heating_entities": [
            "str"
        ],
        "ha_access_token": "str?",
        "decision_interval": "int(30,600)",
        "enable_gpu": "bool"
    }
}