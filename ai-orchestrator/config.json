{
    "name": "AI Orchestrator",
    "version": "0.9.9",
    "slug": "ai-orchestrator",
    "description": "Phase 6: Universal Agents, RAG, and No-Code Factory for Home Assistant",
    "url": "https://github.com/ITSpecialist111/HASS-AI-Orchestrator",
    "arch": [
        "amd64",
        "armv7",
        "aarch64"
    ],
    "startup": "application",
    "boot": "auto",
    "init": false,
    "ingress": true,
    "ingress_port": 8999,
    "ingress_entry": "/",
    "panel_icon": "mdi:robot",
    "ports": {
        "8999/tcp": 8999,
        "3654/tcp": 3654
    },
    "ports_description": {
        "8999/tcp": "Backend API and Dashboard",
        "3654/tcp": "Development dashboard server (optional)"
    },
    "map": [
        "addon_config:rw"
    ],
    "hassio_api": true,
    "hassio_role": "default",
    "homeassistant_api": true,
    "options": {
        "ollama_host": "http://localhost:11434",
        "dry_run_mode": true,
        "log_level": "info",
        "ha_access_token": "",
        "decision_interval": 120,
        "enable_gpu": false,
        "smart_model": "deepseek-r1:8b",
        "fast_model": "mistral:7b-instruct",
        "orchestrator_model": "deepseek-r1:8b",
        "allowed_domains": "light,switch,fan,climate,media_player,cover,input_boolean,input_select,input_number,scene,button,vacuum,water_heater,lock,alarm_control_panel,camera",
        "blocked_domains": "shell_command,hassio,script,automation,rest_command",
        "high_impact_services": "lock.unlock,lock.lock,alarm_control_panel.alarm_disarm,alarm_control_panel.alarm_arm_home,alarm_control_panel.alarm_arm_away,camera.disable_motion_detection,camera.turn_off",
        "min_temp": 10.0,
        "max_temp": 30.0,
        "max_temp_change": 3.0
    },
    "schema": {
        "ollama_host": "str",
        "dry_run_mode": "bool",
        "log_level": "list(debug|info|warning|error)",
        "ha_access_token": "str?",
        "decision_interval": "int(30,600)",
        "enable_gpu": "bool",
        "smart_model": "str",
        "fast_model": "str",
        "orchestrator_model": "str",
        "allowed_domains": "str",
        "blocked_domains": "str",
        "high_impact_services": "str",
        "min_temp": "float",
        "max_temp": "float",
        "max_temp_change": "float"
    }
}